{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "@next/next/no-document-import-in-page": "off",
    "@next/next/no-img-element": "off",
    // Checks rules of Hooks
    "react-hooks/rules-of-hooks": "error",
    // Checks effect dependencies},
    "react-hooks/exhaustive-deps": "warn"
  }
}
